{
    "manifest_version": 2,

    "name": "Webpage Editer",
    "description": " Webpage Editer makes Webpages editable",
    "version": "1.0",

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],

    "browser_action": {
        "default_popup": "popup.html"
    }
}